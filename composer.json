{
    "name": "omegacms/mail",
    "type": "library",
    "description": "Omega CMS Mail Package",
    "keywords": [ "omegacms", "framework", "mail" ],
    "homepage": "https://omegacms.github.io",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Adriano Giovannini",
            "email": "agisoftt@gmail.com",
            "homepage": "https://omegacms.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "egulias/email-validator": "^2.0|^3.1",
        "symfony/polyfill-iconv": "^1.0",
        "symfony/polyfill-mbstring": "^1.0",
        "symfony/polyfill-intl-idn": "^1.10"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "symfony/phpunit-bridge": "^4.4|^5.4"
    },
    "suggest": {
        "ext-intl": "Needed to support internationalized email addresses"
    },
    "autoload": {
        "files": ["src/swift_required.php"]
    },
    "autoload-dev": {
        "psr-0": { "Swift_": "tests/unit" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
